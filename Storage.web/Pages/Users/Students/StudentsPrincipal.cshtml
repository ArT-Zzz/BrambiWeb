@page
@using Microsoft.EntityFrameworkCore
@using Microsoft.EntityFrameworkCore.ChangeTracking
@using AutoGens
@{
    ViewData["Title"] = "Student Menu";
    ViewData["ShowNavBar"] = true;
    string username = TempData["Username"]?.ToString() ?? "11111111";
    var userTypeAndUsername = Tuple.Create("Estudiante", username);
}

<div class="container">
    <!-- Incluir la vista parcial y pasar el modelo -->
    <partial name="_NavBar" model="userTypeAndUsername" />
</div>
<h1>Welcome Student! Please choose any of the options above</h1>
