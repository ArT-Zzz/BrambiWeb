@page
@{
    string username = TempData["Username"]?.ToString() ?? "11111111";
    ViewData["ShowNavBar"] = true;
    var userTypeAndUsername = Tuple.Create("Coordinador", username);
}

<div class="container">
    <div class="container">
        <!-- Incluir la vista parcial y pasar el modelo -->
        <partial name="_NavBar" model="userTypeAndUsername" />
    </div>
    <h1 class="display-4">Group CRUD Menu</h1>

    <div class="d-inline-block">
        <form method="get" action="/Users/Coordinator/CoordinatorActions/GroupsCRUD/ViewGroups">
            <button type="submit" class="btn btn-primary">View Groups</button>
        </form>
    </div>

    <div class="d-inline-block">
        <form method="get" action="/Users/Coordinator/CoordinatorActions/GroupsCRUD/UpdateGroups">
            <button type="submit" class="btn btn-primary">Update Groups</button>
        </form>
    </div>
</div>
