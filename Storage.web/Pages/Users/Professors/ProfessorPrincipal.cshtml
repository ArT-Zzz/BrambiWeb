@page
@using Microsoft.EntityFrameworkCore
@using Microsoft.EntityFrameworkCore.ChangeTracking
@using AutoGens
@{
    // Establecer el título de la página
    ViewData["Title"] = "Professor Menu";
    string username = TempData["Username"]?.ToString() ?? "11111111";
    ViewData["ShowNavBar"] = true;
    var userTypeAndUsername = Tuple.Create("Profesor", username);
}

<div class="container">
    <!-- Incluir la vista parcial y pasar el modelo -->
    <partial name="_NavBar" model="userTypeAndUsername" />
</div>
<h1>Welcome Professor! Please choose any of the options above</h1>
