@page
@{
    string username = TempData["Username"]?.ToString() ?? "11111111";
    ViewData["ShowNavBar"] = true;
    var userTypeAndUsername = Tuple.Create("Coordinador", username);
}
<div class="container">
    <div class="container">
        <!-- Incluir la vista parcial y pasar el modelo -->
        <partial name="_NavBar" model="userTypeAndUsername" />
    </div>
    <h1 class="display-4">Storer CRUD Menu</h1>

    <div class="d-inline-block">
        <form method="get" action="/Users/Storer/CRUD/ViewStorer">
            <button type="submit" class="btn btn-primary">View Storers</button>
        </form>
    </div>

    <div class="d-inline-block">
        <form method="get" action="/Users/Storer/CRUD/UpdateStorer">
            <button type="submit" class="btn btn-primary">Update Storer</button>
        </form>
    </div>

    <div class="d-inline-block">
        <form method="get" action="/Users/Storer/CRUD/DeleteStorer">
            <button type="submit" class="btn btn-primary">Delete Storer</button>
        </form>
    </div>
</div>
